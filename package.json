{
  "name": "new-rasa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --config vite.config.analyzer.ts",
    "build:stats": "vite build --mode production --outDir dist-stats && npx serve dist-stats",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "perf:audit": "npm run build && npx lighthouse http://localhost:4173 --output=html --output-path=./reports/lighthouse.html",
    "bundle:size": "npm run build && npx bundlesize",
    "clean": "rm -rf dist dist-stats node_modules/.cache"
  },
